{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-youtube-plugin/ckeditor-youtube-plugin",
                "version": "2.1.13",
                "type": "drupal-library",
                "dist": {
                  "url": "https://github.com/fonini/ckeditor-youtube-plugin/archive/v2.1.13.zip",
                  "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "bootstrap-datepicker/bootstrap-datepicker",
                "version": "1.9.0",
                "type": "drupal-library",
                "dist": {
                  "url": "https://github.com/uxsolutions/bootstrap-datepicker/archive/master.zip",
                  "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "datetimepicker/datetimepicker",
                "version": "2.5.20",
                "type": "drupal-library",
                "dist": {
                  "url": "https://github.com/xdan/datetimepicker/archive/2.5.20.zip",
                  "type": "zip"
                }
            }
		},
		{
			"type": "package",
			"package": {
				"name": "furf/jquery-ui-touch-punch",
				"version": "dev-master",
				"type": "drupal-library",
				"dist": {
					"url": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip",
					"type": "zip"
				}
			}	
        }
    ],
    "require": {
        "php": ">=5.6",
        "bootstrap-datepicker/bootstrap-datepicker": "^1.9",
        "ckeditor-youtube-plugin/ckeditor-youtube-plugin": "^2.1",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "datetimepicker/datetimepicker": "^2.5",
        "drupal/admin_toolbar": "3.3.0",
        "drupal/advagg": "6.0.0-alpha1",
        "drupal/allowed_formats": "^2.0",
        "drupal/autologout": "1.4",
        "drupal/better_exposed_filters": "6.0.3",
        "drupal/block_visibility_groups": "2.0.1",
        "drupal/bootstrap_barrio": "5.1.10",
        "drupal/bootstrap_datepicker": "2.0.0",
        "drupal/bootstrap_sass": "5.0.6",
        "drupal/ckeditor": "^1.0",
        "drupal/ckeditor_youtube": "1.5",
        "drupal/ckwordcount": "^2.0",
        "drupal/conditional_fields": "4.0.0-alpha2",
        "drupal/config_split": "1.9",
        "drupal/cookiehub": "^1.0",
        "drupal/core": "10.0.7",
        "drupal/ctools": "3.13",
        "drupal/devel": "5.1.1",
        "drupal/devel_debug_log": "2.0.2",
        "drupal/embed": "1.6",
        "drupal/entity_browser": "2.9",
        "drupal/entity_clone": "2.0.0-beta2",
        "drupal/eu_cookie_compliance": "^1.19",
        "drupal/field_group": "3.4",
        "drupal/filelog": "2.1.1",
        "drupal/flood_control": "2.3.2",
        "drupal/geolocation": "3.12",
        "drupal/google_tag": "1.6",
        "drupal/group": "^1.0@RC",
        "drupal/honeypot": "^2.0.2",
        "drupal/image_effects": "3.4",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui_touch_punch": "^1.0",
        "drupal/link_attributes": "^1.5",
        "drupal/linkit": "6.0.0-beta4",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/menu_block": "^1.5",
        "drupal/menu_trail_by_path": "2.0.0",
        "drupal/metatag": "^1.8",
        "drupal/office_hours": "1.8",
        "drupal/pathauto": "1.11",
        "drupal/pathologic": "^2.0@dev",
        "drupal/permissions_filter": "^1.0",
        "drupal/quick_node_clone": "1.16",
        "drupal/recaptcha": "^3.1",
        "drupal/recaptcha_v3": "^1.7",
        "drupal/redirect": "^1.3",
        "drupal/redis": "^1.1",
        "drupal/restui": "^1.17",
        "drupal/role_delegation": "^1.0@alpha",
        "drupal/scheduler": "^1.0",
        "drupal/search_api": "^1.28",
        "drupal/sendgrid_integration": "2.1",
        "drupal/simple_sitemap": "4.1.4",
        "drupal/single_datetime": "2.0.0-beta1",
        "drupal/spamspan": "3.0.0-beta2",
        "drupal/time_range": "2.0.1",
        "drupal/token": "^1.5",
        "drupal/tvi": "2.0.0-alpha1",
        "drupal/twig_debugger": "^1.1",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/upgrade_status": "4.0.0",
        "drupal/webform": "6.2.0-beta5",
        "drush/drush": "^11.0",
        "furf/jquery-ui-touch-punch": "dev-master",
        "platformsh/config-reader": "^2.2",
        "vlucas/phpdotenv": "^2.4",
        "w8tcha/ckeditor-wordcount-plugin": "^1.17",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^2.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/console-extend-plugin": true,
            "zodiacmedia/drupal-libraries-installer": true,
            "cweagans/composer-patches": true,
            "wikimedia/composer-merge-plugin": true,
            "zaporylie/composer-drupal-optimizations": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/core": {
                "Grouped exposed filters do not work like they should": "https://www.drupal.org/files/issues/1810148-58.patch",
                "Untranslatable aliases generation fix for translations": "https://www.drupal.org/files/issues/2022-12-20/2689459-161.patch",
				"Entity view/form mode formatter/widget settings have no translation UI Action patch": "https://www.drupal.org/files/issues/2020-12-04/2546212-168-actions.patch",
				"Entity view/form mode formatter/widget settings have no translation UI": "https://www.drupal.org/files/issues/2020-12-04/2546212-168.patch",
				"PHP Notice: Undefined index: type in claro_preprocess_input()": "https://www.drupal.org/files/issues/2020-06-02/preprocess-input-notice-3144876-6.patch"
            },
			"drupal/redirect_after_login": {
				"Headers have already been sent after upgrade to Drupal 9.2 (can't login)": "https://www.drupal.org/files/issues/2021-06-20/3214949.patch"
			},
			"drupal/search_api": {
				"Search Results - undefined array key #conjunction in prepareKeys()": "patches/contrib/search_api/0001-fix.patch"
			},
			"drupal/cookiehub": {
				"Cookiehub language": "patches/contrib/cookiehub/cookiehub-language-v2.patch"
			},
			"drupal/inline_entity_form": {
				"Translate entity type label": "https://www.drupal.org/files/issues/2021-01-28/inline_entity_form-translate-entity-type-label-3195264-2-D8.patch"
			},
			"drupal/shortcutperrole": {
				"Automated Drupal 10 patch": "https://www.drupal.org/files/issues/2022-06-16/shortcutperrole.2.0.x-dev.rector.patch"
			},
			"drupal/force_password_change": {
				"Automated Drupal 10 patch": "https://www.drupal.org/files/issues/2022-07-22/force_password_change.2.0.x-dev.rector.patch"
			}
			
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
		"merge-plugin": {
            "include": [
                "web/modules/contrib/ckwordcount/composer.libraries.json"
            ]
        }
    }
}
