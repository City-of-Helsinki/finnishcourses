{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-youtube-plugin/ckeditor-youtube-plugin",
                "version": "2.1.13",
                "type": "drupal-library",
                "dist": {
                  "url": "https://github.com/fonini/ckeditor-youtube-plugin/archive/v2.1.13.zip",
                  "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "bootstrap-datepicker/bootstrap-datepicker",
                "version": "1.9.0",
                "type": "drupal-library",
                "dist": {
                  "url": "https://github.com/uxsolutions/bootstrap-datepicker/archive/master.zip",
                  "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "datetimepicker/datetimepicker",
                "version": "2.5.20",
                "type": "drupal-library",
                "dist": {
                  "url": "https://github.com/xdan/datetimepicker/archive/2.5.20.zip",
                  "type": "zip"
                }
            }
		},
		{
			"type": "package",
			"package": {
				"name": "furf/jquery-ui-touch-punch",
				"version": "dev-master",
				"type": "drupal-library",
				"dist": {
					"url": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip",
					"type": "zip"
				}
			}	
        }
    ],
    "require": {
        "php": ">=5.6",
        "bootstrap-datepicker/bootstrap-datepicker": "^1.9",
        "ckeditor-youtube-plugin/ckeditor-youtube-plugin": "^2.1",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "datetimepicker/datetimepicker": "^2.5",
        "drupal/admin_toolbar": "3.0.2",
        "drupal/advagg": "4.1",
        "drupal/allowed_formats": "1.3",
        "drupal/autologout": "1.3",
        "drupal/better_exposed_filters": "^5.0@alpha",
        "drupal/block_visibility_groups": "1.4",
        "drupal/bootstrap_barrio": "^4.22",
        "drupal/bootstrap_datepicker": "^1.0@dev",
        "drupal/bootstrap_sass": "^1.9",
        "drupal/business_rules": "^2.0@beta",
        "drupal/ckeditor_youtube": "1.3",
        "drupal/ckeditorheight": "1.8",
        "drupal/conditional_fields": "4.0.0-alpha1",
        "drupal/config_split": "1.7",
        "drupal/console": "^1.8",
        "drupal/cookies": "^1.0",
        "drupal/core": "9.2.8",
        "drupal/ctools": "3.7",
        "drupal/dbug": "^2.0",
        "drupal/delete_all": "2.0.0",
        "drupal/devel_debug_log": "^1.0@beta",
        "drupal/embed": "1.4",
        "drupal/entity_browser": "2.6",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/entity_embed": "1.2.0",
        "drupal/eu_cookie_compliance": "^1.19",
        "drupal/facets": "1.8",
        "drupal/field_group": "3.1",
        "drupal/filelog": "2.0",
        "drupal/flood_control": "2.2.2",
        "drupal/force_password_change": "^2.0@beta",
        "drupal/geolocation": "3.7",
        "drupal/google_tag": "1.4",
        "drupal/group": "^1.0@RC",
        "drupal/honeypot": "2.0.1",
        "drupal/image_effects": "3.1",
        "drupal/jquery_ui_touch_punch": "^1.0",
        "drupal/link_attributes": "^1.5",
        "drupal/linkit": "5.0.0-beta13",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/menu_block": "^1.5",
        "drupal/menu_trail_by_path": "^1.1",
        "drupal/metatag": "^1.8",
        "drupal/office_hours": "1.x",
        "drupal/pathauto": "1.8",
        "drupal/pathologic": "^1.0@alpha",
        "drupal/permissions_filter": "^1.0",
        "drupal/quick_node_clone": "^1.12",
        "drupal/redirect": "^1.3",
        "drupal/redirect_after_login": "2.7",
        "drupal/redis": "^1.1",
        "drupal/restui": "^1.17",
        "drupal/role_delegation": "^1.0@alpha",
        "drupal/scheduler": "^1.0",
        "drupal/search_api": "1.20",
        "drupal/select_or_other": "1.x-dev",
        "drupal/shortcutperrole": "2.0.0",
        "drupal/simple_sitemap": "^3.2",
        "drupal/single_datetime": "^1.7",
        "drupal/spamspan": "^1.2",
        "drupal/system_status": "^2.8",
        "drupal/taxonomy_delete": "^1.0",
        "drupal/time_range": "9.0.0",
        "drupal/token": "^1.5",
        "drupal/tvi": "^1.0@beta",
        "drupal/twig_debugger": "^1.1",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/upgrade_status": "3.11",
        "drupal/view_unpublished": "^1.0@alpha",
        "drupal/webform": "6.1.0",
        "drush/drush": "^10.5",
        "furf/jquery-ui-touch-punch": "dev-master",
        "platformsh/config-reader": "^2.2",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/core": {
                "Grouped exposed filters do not work like they should": "https://www.drupal.org/files/issues/1810148-58.patch",
                "Untranslatable aliases generation fix for translations": "https://www.drupal.org/files/issues/2020-01-26/2689459-127.patch",
				"Attribute hreflang not allowed on element li at this point": "https://www.drupal.org/files/issues/2020-12-14/D9-2454289-101.patch"
            },
			"drupal/redirect_after_login": {
				"Headers have already been sent after upgrade to Drupal 9.2 (can't login)": "https://www.drupal.org/files/issues/2021-06-20/3214949.patch"
			}
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
